system  Bag1, Controler, FeedBelt1, DistributionBelt, User;</system>
	<queries>
		<query>
			<formula>A[] not deadlock</formula>
			<comment>deadlock check </comment>
		</query>
		<query>
			<formula>E&lt;&gt; Bag1.Destination_A</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; Bag1.Destination_B</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; (Bag1.Destination_A and Bag1.x &lt;52)</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; (Bag1.Destination_A and Bag1.x &gt;=79)</formula>
			<comment></comment>
		</query>
		<query>
			<formula>A&lt;&gt; (Bag1.Destination_A || Bag1.Destination_B )</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
